class person:
    def __init__(self,name,age,city):
        self.name=name
        self.age=age
        self.city=city
    def print_details(self):
            print("name:",self.name)
            print("age:",self.age)
            print("city:",self.city)
    def change_city(self):
         self.city=input("enter ur new city:")
p1=person("lavi",16,"noida")
p2=person("sona",14,"delhi")
p1.print_details()
p2.print_details()
p1.change_city()
p1.print_details()
p2.print_details()
