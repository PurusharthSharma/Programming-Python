numbers=[12,78,67,45,78,90]
print("Traversing list using forward indexing")
for i in range(len(numbers)):
    print(numbers[i],end="")
print("\nTraversing list using backward indexing")
for i in range(-1,-len(numbers)-1,-1):
    print(numbers[i],end="")
print("\nTraversing using without indexing")
for number in numbers:
    print(number,end="")
